{
  "Hario V60": {
    "defaultGrindSize": "Medium",
    "steps": [
      {
        "id": 1,
        "StepTitle": "Boil some water",
        "SvgPath": "Kettle",
        "SubText": "Boil more than you need",
        "isTimedStep": false
      },
      {
        "id": 2,
        "StepTitle": "Grind ${coffeeInput}g of your coffee",
        "SvgPath": "Grinder",
        "SubText": "Grind on ${grindSize}",
        "isTimedStep": false
      },
      {
        "id": 3,
        "StepTitle": "Wet your filter with hot water",
        "SvgPath": "wetFilter",
        "SubText": "Dump the water out afterwatrds",
        "isTimedStep": false
      },
      {
        "id": 4,
        "StepTitle": "Put your coffee grounds in the filter",
        "SvgPath": "fillFilter",
        "SubText": "Make sure the bed of grounds is level",
        "isTimedStep": false
      },
      {
        "id": 5,
        "StepTitle": "Tare your scale back to 0",
        "SvgPath": "Scale",
        "SubText": "Get ready to brew!",
        "isTimedStep": false
      },
      {
        "id": 1,
        "StepTitle": "Quickly pour 50g of water",
        "SvgPath": "wetFilter",
        "SubText": "Make sure to evenly wet the coffee grounds",
        "isTimedStep": true,
        "time": 15
      },
      {
        "id": 2,
        "StepTitle": "Let stand for ~30 seconds",
        "SvgPath": "Clock",
        "SubText": "We're letting the coffee release it's gas",
        "isTimedStep": true,
        "time": 45
      },
      {
        "id": 3,
        "StepTitle": "Pour ${Math.floor((coffeeOutput * 29.57 -50)/2)}g of water",
        "SvgPath": "wetFilter",
        "SubText": "Make sure to pour in even circles!",
        "isTimedStep": true,
        "time": 70
      },
      {
        "id": 4,
        "StepTitle": "Wait until the water level drops roughly an inch",
        "SvgPath": "Clock",
        "SubText": "It's always a great time to have coffee!",
        "isTimedStep": true,
        "time": 80
      },
      {
        "id": 5,
        "StepTitle": "Pour until you've reached ${Math.floor(coffeeOutput * 29.57)}g of water",
        "SvgPath": "wetFilter",
        "SubText": "Avoid hitting the walls of your filter",
        "isTimedStep": true,
        "time": 110
      },
      {
        "id": 6,
        "StepTitle": "Wait until the water stops dripping",
        "SvgPath": "Clock",
        "SubText": "Stop the timer when it's finished",
        "isTimedStep": true,
        "time": 125
      }
    ]
  },
